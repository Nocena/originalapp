    "lint:ts:fix": "eslint --fix \"src/**/*.{ts,tsx}\"",
    "lint:reset-cache": "eslint --cache --cache-location .eslintcache --fix .",
    "start:https": "node https-server.js",
    "format": "pnpm run format:check",
    "format:check": "prettier '**/*.{js,jsx,ts,tsx}' --config .prettierrc.json --check",
    "format:fix": "prettier '**/*.{js,jsx,ts,tsx}' --config .prettierrc.json --write",
    "challenge:daily": "tsx src/scripts/generateDailyChallenge.ts",
    "challenge:weekly": "tsx src/scripts/generateWeeklyChallenge.ts",
    "challenge:monthly": "tsx src/scripts/generateMonthlyChallenge.ts",
    "dgraph:codegen": "graphql-codegen --config src/lib/dgraph/codegen.ts"
  },
